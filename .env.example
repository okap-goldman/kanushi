# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_project_url
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# API Keys
OPENROUTER_API_KEY=your_openrouter_api_key

# Backblaze B2 Storage (for media uploads)
B2_KEY_ID=your_b2_key_id
B2_APPLICATION_KEY=your_b2_application_key
B2_BUCKET_ID=your_b2_bucket_id
B2_BUCKET_NAME=your_b2_bucket_name

# Cloudflare Configuration (for CDN)
CLOUDFLARE_CDN_URL=your_cloudflare_cdn_url
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token

# Application Configuration
APP_NAME=神主 (Kanushi)
APP_SCHEME=kanushi
APP_VERSION=1.0.0

# Environment
NODE_ENV=development

# Push Notifications (optional)
EXPO_PUBLIC_PUSH_NOTIFICATION_SERVER_URL=your_push_notification_server_url

# Analytics (optional)
EXPO_PUBLIC_ANALYTICS_API_KEY=your_analytics_api_key

# Sentry Error Tracking (optional)
SENTRY_DSN=your_sentry_dsn
SENTRY_ORG=your_sentry_org
SENTRY_PROJECT=your_sentry_project
SENTRY_AUTH_TOKEN=your_sentry_auth_token

# Feature Flags
EXPO_PUBLIC_ENABLE_ANALYTICS=false
EXPO_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=false
EXPO_PUBLIC_ENABLE_ERROR_TRACKING=false